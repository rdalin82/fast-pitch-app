<div class="jumbo">
  <div class="container">
    <h1 class="block-title">Form Responses</h1>
  </div>
</div>
<div class="container">
  <div>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Presenter Name</th>
          <th scope="col">Coach</th>
          <th scope="col">Question</th>
          <th scope="col">Points for Question</th>

        </tr>
      </thead>
      <%@scores.each do |score|%>
        <tbody>
          <tr>
            <td><%=score.presenter.name%></td>
            <td><%=score.user.fullname%></td>
            <td><%=score.question.content%></td>
            <td><%=score.points%></td>
          </tr>
        </tbody>
      <%end%>
    </table>
    <a href="/pages/csv_form_responses.csv">
      <button class="btn btn-success">export table </button>
    </a>
  </div>
</div>

